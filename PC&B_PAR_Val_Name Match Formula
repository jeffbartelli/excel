              MID(
                SUBSTITUTE(
                  SUBSTITUTE(
                    SUBSTITUTE(
                      SUBSTITUTE(
                        SUBSTITUTE(K3," JR",""),
                      " SR",""),
                    " II",""),
                  " III",""),
                " IV",""),
